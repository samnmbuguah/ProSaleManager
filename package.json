{
  "name": "prosalemanager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest tests/e2e",
    "test:e2e:watch": "NODE_OPTIONS=--experimental-vm-modules jest tests/e2e --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "bcryptjs": "^3.0.2",
    "chromedriver": "^138.0.3",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "selenium-webdriver": "^4.33.0",
    "supertest": "^7.1.3",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "vitest": "^3.2.4"
  }
}
