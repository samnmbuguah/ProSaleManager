{
  "apps": [
    {
      "name": "eltee-store-server",
      "script": "dist/src/index.js",
      "cwd": "/home/elteijae/eltee_store",
      "instances": 1,
      "exec_mode": "fork",
      "env": {
        "NODE_ENV": "production",
        "PORT": 5000
      },
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "log_file": "./logs/combined.log",
      "time": true,
      "merge_logs": true,
      "restart_delay": 4000,
      "max_restarts": 5,
      "min_uptime": "10s"
    }
  ]
}
